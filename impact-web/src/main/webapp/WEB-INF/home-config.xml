<!DOCTYPE faces-config PUBLIC
    "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
    "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config xmlns="http://java.sun.com/JSF/Configuration">
  <!--Home Tab-->
  
  <!-- menu items -->
  <managed-bean>
    <managed-bean-name>menuItem_home</managed-bean-name>
    <managed-bean-class>
      us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    <managed-property>
      <property-name>beanName</property-name>
      <value>menuItem_home</value>
    </managed-property>
    <managed-property>
      <property-name>label</property-name>
      <value>Home</value>
    </managed-property>
    <managed-property>
      <property-name>name</property-name>
      <value>home</value>
    </managed-property>
    <managed-property>
      <property-name>viewIDs</property-name>
      <list-entries>
        <value-class>java.lang.String</value-class>
        <value>/home/home.jsp</value>
      </list-entries>
    </managed-property>
    <managed-property>
      <property-name>children</property-name>
      <list-entries>
        <value-class>
          us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem</value-class>
        <value>#{menuItem_todo}</value>
        <value>#{menuItem_permitToDo}</value>
        <value>#{menuItem_initTask}</value>
      </list-entries>
    </managed-property>
    <managed-property>
      <property-name>outcome</property-name>
      <value>method.toDoSearch.submitHome</value>
    </managed-property>
  </managed-bean>
  
  <managed-bean>
    <managed-bean-name>menuItem_todo</managed-bean-name>
    <managed-bean-class>
      us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
     <managed-property>
      <property-name>beanName</property-name>
      <value>menuItem_todo</value>
    </managed-property>
    <managed-property>
      <property-name>label</property-name>
      <value>To Dos</value>
    </managed-property>
    <managed-property>
      <property-name>name</property-name>
      <value>home.todo</value>
    </managed-property>
    <managed-property>
      <property-name>viewIDs</property-name>
      <list-entries>
        <value-class>java.lang.String</value-class>
        <value>/home/home.jsp</value>
      </list-entries>
    </managed-property>
    <managed-property>
      <property-name>children</property-name>
      <list-entries>
        <value-class>
          us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem</value-class>
      </list-entries>
    </managed-property>
    <managed-property>
      <property-name>outcome</property-name>
      <value>method.toDoSearch.submitHome</value>
    </managed-property>
  </managed-bean>
  
  <managed-bean>
    <managed-bean-name>menuItem_permitToDo</managed-bean-name>
    <managed-bean-class>
      us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
     <managed-property>
      <property-name>beanName</property-name>
      <value>menuItem_permitToDo</value>
    </managed-property>
    <managed-property>
      <property-name>label</property-name>
      <value>Permit To Dos</value>
    </managed-property>
    <managed-property>
      <property-name>name</property-name>
      <value>home.permitToDo</value>
    </managed-property>
    <managed-property>
      <property-name>viewIDs</property-name>
      <list-entries>
        <value-class>java.lang.String</value-class>
        <value>/home/permitToDo.jsp</value>
      </list-entries>
    </managed-property>
    <managed-property>
      <property-name>children</property-name>
      <list-entries>
        <value-class>
          us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem</value-class>
      </list-entries>
    </managed-property>
    <managed-property>
      <property-name>outcome</property-name>
      <value>method.permitToDoSearch.submit</value>
    </managed-property>
  </managed-bean>
  
  <managed-bean>
    <managed-bean-name>menuItem_initTask</managed-bean-name>
    <managed-bean-class>
      us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
     <managed-property>
      <property-name>beanName</property-name>
      <value>menuItem_initTask</value>
    </managed-property>
    <managed-property>
      <property-name>label</property-name>
      <value>Create To Do Task</value>
    </managed-property>
    <managed-property>
      <property-name>name</property-name>
      <value>home.initTask</value>
    </managed-property>
    <managed-property>
      <property-name>viewIDs</property-name>
      <list-entries>
        <value-class>java.lang.String</value-class>
        <value>/home/initTask.jsp</value>
      </list-entries>
    </managed-property>
    <managed-property>
      <property-name>outcome</property-name>
      <value>initTask</value>
    </managed-property>
  </managed-bean>
  
  <managed-bean>
    <managed-bean-name>menuItem_initWorkflow</managed-bean-name>
    <managed-bean-class>
      us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
     <managed-property>
      <property-name>beanName</property-name>
      <value>menuItem_initWorkflow</value>
    </managed-property>
    <managed-property>
      <property-name>label</property-name>
      <value>Init Workflow</value>
    </managed-property>
    <managed-property>
      <property-name>name</property-name>
      <value>home.initWorkflow</value>
    </managed-property>
    <managed-property>
      <property-name>viewIDs</property-name>
      <list-entries>
        <value-class>java.lang.String</value-class>
        <value>/home/initWorkflow.jsp</value>
      </list-entries>
    </managed-property>
    <managed-property>
      <property-name>outcome</property-name>
      <value>initWorkflow</value>
    </managed-property>
  </managed-bean>
  
  <managed-bean>
    <managed-bean-name>menuItem_eventLog</managed-bean-name>
    <managed-bean-class>
      us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
     <managed-property>
      <property-name>beanName</property-name>
      <value>menuItem_eventLog</value>
    </managed-property>
    <managed-property>
      <property-name>label</property-name>
      <value>Event Log</value>
    </managed-property>
    <managed-property>
      <property-name>name</property-name>
      <value>home.eventLog</value>
    </managed-property>
    <managed-property>
      <property-name>viewIDs</property-name>
      <list-entries>
        <value-class>java.lang.String</value-class>
        <value>/home/eventLog.jsp</value>
      </list-entries>
    </managed-property>
    <managed-property>
      <property-name>children</property-name>
      <list-entries>
        <value-class>
          us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem</value-class>
        <value>#{menuItem_eventNew}</value>
      </list-entries>
    </managed-property>
    <managed-property>
      <property-name>outcome</property-name>
      <value>eventLog</value>
    </managed-property>
  </managed-bean>
  
  <managed-bean>
    <managed-bean-name>menuItem_eventNew</managed-bean-name>
    <managed-bean-class>
      us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
     <managed-property>
      <property-name>beanName</property-name>
      <value>menuItem_eventNew</value>
    </managed-property>
    <managed-property>
      <property-name>label</property-name>
      <value>New Event</value>
    </managed-property>
    <managed-property>
      <property-name>name</property-name>
      <value>home.eventNew</value>
    </managed-property>
    <managed-property>
      <property-name>viewIDs</property-name>
      <list-entries>
        <value-class>java.lang.String</value-class>
        <value>/home/eventNew.jsp</value>
      </list-entries>
    </managed-property>
    <managed-property>
      <property-name>outcome</property-name>
      <value>eventNew</value>
    </managed-property>
  </managed-bean>
  <!-- end menu items -->
  
  
  
  <navigation-rule>
    <navigation-case>
      <from-outcome>todo</from-outcome>
      <to-view-id>/home/home.jsp</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>permitToDo</from-outcome>
      <to-view-id>/home/permitToDo.jsp</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>eventLog</from-outcome>
      <to-view-id>/home/eventLog.jsp</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>eventNew</from-outcome>
      <to-view-id>/home/eventNew.jsp</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>initTask</from-outcome>
      <to-view-id>/home/initTask.jsp</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>initWorkflow</from-outcome>
      <to-view-id>/home/initWorkflow.jsp</to-view-id>
      <redirect/>
    </navigation-case>
  </navigation-rule>
  
  <managed-bean>
    <managed-bean-name>initWorkflow</managed-bean-name>
    <managed-bean-class> us.oh.state.epa.stars2.app.workflow.InitWorkflow
      </managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>readWorkFlowService</property-name>
            <value>#{readWorkFlowBO}</value>
        </managed-property>
        <managed-property>
            <property-name>facilityService</property-name>
            <value>#{facilityBO}</value>
        </managed-property>
  </managed-bean>
  
  
  <managed-bean>
    <managed-bean-name>toDoSearch</managed-bean-name>
    <managed-bean-class> us.oh.state.epa.stars2.app.workflow.ToDoSearch
      </managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>workFlowService</property-name>
            <value>#{readWorkFlowBO}</value>
        </managed-property>
        <managed-property>
            <property-name>complianceReportService</property-name>
            <value>#{complianceReportBO}</value>
        </managed-property>
        <managed-property>
            <property-name>stackTestService</property-name>
            <value>#{stackTestBO}</value>
        </managed-property>
        <managed-property>
            <property-name>emissionsReportService</property-name>
            <value>#{emissionsReportBO}</value>
        </managed-property>
         <managed-property>
            <property-name>monitoringService</property-name>
            <value>#{monitoringBO}</value>
        </managed-property>
        <managed-property>
            <property-name>enforcementActionService</property-name>
            <value>#{enforcementActionBO}</value>
        </managed-property>
        <managed-property>
            <property-name>fullComplianceEvalService</property-name>
            <value>#{fullComplianceEvalBO}</value>
        </managed-property>
  </managed-bean>
  
  
  <managed-bean>
    <managed-bean-name>permitToDoSearch</managed-bean-name>
    <managed-bean-class> us.oh.state.epa.stars2.app.home.PermitToDoSearch
      </managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>workFlowService</property-name>
            <value>#{readWorkFlowBO}</value>
        </managed-property>
        <managed-property>
            <property-name>permitService</property-name>
            <value>#{permitBO}</value>
        </managed-property>
  </managed-bean>
  
  
  <managed-bean>
    <managed-bean-name>eventLog</managed-bean-name>
    <managed-bean-class> us.oh.state.epa.stars2.app.workflow.EventLog
      </managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>facilityService</property-name>
            <value>#{facilityBO}</value>
        </managed-property>
  </managed-bean>
  
  
  <navigation-rule>
    <from-view-id>/home/home.jsp</from-view-id>
    <navigation-case>
      <from-action>#{toDoSearch.submit}</from-action>
      <from-outcome>Success</from-outcome>
      <to-view-id>/home/home.jsp</to-view-id>
    </navigation-case>
    
    <navigation-case>
      <from-action>#{facilityProfile.submitProfile}</from-action>
      <from-outcome>Success</from-outcome>
      <to-view-id>/facilities/facilityProfile.jsp</to-view-id>
      <redirect/>
    </navigation-case>

    <navigation-case>
      <from-action>#{workFlow2DDraw.submitProfile}</from-action>
      <from-outcome>Success</from-outcome>
      <to-view-id>/workflows/workflowProfile.jsp</to-view-id>
      <redirect/>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/home/eventNew.jsp</from-view-id>
    <navigation-case>
      <from-action>#{eventLog.createEvent}</from-action>
      <from-outcome>Success</from-outcome>
      <to-view-id>/home/eventLog.jsp</to-view-id>
      <redirect/>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/home/initWorkflow.jsp</from-view-id>
    <navigation-case>
      <from-action>#{initWorkflow.submit}</from-action>
      <from-outcome>Success</from-outcome>
      <to-view-id>/home/initWorkflow.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/home/initTask.jsp</from-view-id>
    <navigation-case>
      <from-action>#{initWorkflow.submit}</from-action>
      <from-outcome>Success</from-outcome>
      <to-view-id>/home/initTask.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
</faces-config>