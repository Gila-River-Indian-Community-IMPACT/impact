  <navigation-rule>
    <from-view-id>/admin/users.jsp</from-view-id>
    <navigation-case>
      <from-action>#{userTree.editUser}</from-action>
      <from-outcome>Editable</from-outcome>
      <to-view-id>/admin/users.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-action>#{userTree.saveUser}</from-action>
      <from-outcome>Success</from-outcome>
      <to-view-id>/admin/users.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-action>#{userTree.cancelUser}</from-action>
      <from-outcome>Cancelled</from-outcome>
      <to-view-id>/admin/users.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/admin/roles.jsp</from-view-id>
    <navigation-case>
      <from-action>#{roleTree.addNewRole}</from-action>
      <from-outcome>RoleAdded</from-outcome>
      <to-view-id>/admin/roles.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-action>#{roleTree.saveRole}</from-action>
      <from-outcome>RoleSaved</from-outcome>
      <to-view-id>/admin/roles.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-action>#{roleTree.cancel}</from-action>
      <from-outcome>Cancelled</from-outcome>
      <to-view-id>/admin/roles.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-action>#{roleTree.editRole}</from-action>
      <from-outcome>RoleEditable</from-outcome>
      <to-view-id>/admin/roles.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-action>#{roleTree.cloneRole}</from-action>
      <from-outcome>RoleCloned</from-outcome>
      <to-view-id>/admin/roles.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-action>#{roleTree.loadUseCases}</from-action>
      <from-outcome>UseCasesLoaded</from-outcome>
      <to-view-id>/admin/roles.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:confirmRoleDelete</from-outcome>
      <to-view-id>/admin/confirmRoleDelete.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
    <from-view-id>/admin/serviceCatalog.jsp</from-view-id>
    <navigation-case>
      <from-action>#{serviceCatalog.cancelEdit}</from-action>
      <from-outcome>Cancelled</from-outcome>
      <to-view-id>/admin/serviceCatalog.jsp</to-view-id>
