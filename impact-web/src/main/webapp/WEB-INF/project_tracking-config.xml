<!DOCTYPE faces-config PUBLIC
    "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
    "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config xmlns="http://java.sun.com/JSF/Configuration">
	<!--Proj. Tracking Tab -->
	<!-- menu items -->
	<managed-bean>
		<managed-bean-name>menuItem_projectTracking</managed-bean-name>
		<managed-bean-class>
			us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>menuItem_projectTracking</value>
		</managed-property>
		<managed-property>
			<property-name>label</property-name>
			<value>Project Tracking</value>
		</managed-property>
		<managed-property>
			<property-name>name</property-name>
			<value>projectTracking</value>
		</managed-property>
		<managed-property>
			<property-name>children</property-name>
			<list-entries>
				<value-class>
					us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem
				</value-class>
				<value>#{menuItem_projectTrackingSearch}</value>
				<value>#{menuItem_projectTrackingDetail}</value>
				<value>#{menuItem_createProjectTracking}</value>
				<value>#{menuItem_projectTrackingDefinitions}</value>
			</list-entries>
		</managed-property>
		<managed-property>
			<property-name>outcome</property-name>
			<value>projectTrackingSearch</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>menuItem_projectTrackingSearch</managed-bean-name>
		<managed-bean-class>
			us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>menuItem_projectTrackingSearch</value>
		</managed-property>
		<managed-property>
			<property-name>label</property-name>
			<value>Project Tracking Search</value>
		</managed-property>
		<managed-property>
			<property-name>name</property-name>
			<value>projectTracking.projectTrackingSearch</value>
		</managed-property>
		<managed-property>
			<property-name>viewIDs</property-name>
			<list-entries>
				<value-class>java.lang.String</value-class>
				<value>/projectTracking/projectTrackingSearch.jsp</value>
			</list-entries>
		</managed-property>
		<managed-property>
			<property-name>outcome</property-name>
			<value>projectTrackingSearch</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>menuItem_projectTrackingDetail</managed-bean-name>
		<managed-bean-class>
			us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>menuItem_projectTrackingDetail</value>
		</managed-property>
		<managed-property>
			<property-name>label</property-name>
			<value>Project Tracking Detail</value>
		</managed-property>
		<managed-property>
			<property-name>name</property-name>
			<value>projectTracking.projectTrackingDetail</value>
		</managed-property>
		<managed-property>
			<property-name>disabled</property-name>
			<value>true</value>
		</managed-property>
		<managed-property>
			<property-name>viewIDs</property-name>
			<list-entries>
				<value-class>java.lang.String</value-class>
				<value>/projectTracking/projectTrackingDetail.jsp</value>
			</list-entries>
		</managed-property>
		<managed-property>
			<property-name>outcome</property-name>
			<value>method.projectTrackingDetail.refresh</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>menuItem_createProjectTracking</managed-bean-name>
		<managed-bean-class>
			us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>menuItem_createProjectTracking</value>
		</managed-property>
		<managed-property>
			<property-name>label</property-name>
			<value>Create Project Tracking</value>
		</managed-property>
		<managed-property>
			<property-name>name</property-name>
			<value>projectTracking.createProjectTracking</value>
		</managed-property>
		<managed-property>
			<property-name>viewIDs</property-name>
			<list-entries>
				<value-class>java.lang.String</value-class>
				<value>/projectTracking/createProjectTracking.jsp</value>
			</list-entries>
		</managed-property>
		<managed-property>
			<property-name>outcome</property-name>
			<value>method.createProject.refresh</value>
		</managed-property>
	</managed-bean>
	
		<managed-bean>
		<managed-bean-name>menuItem_projectTrackingDefinitions</managed-bean-name>
		<managed-bean-class>
			us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>menuItem_projectTrackingDefinitions</value>
		</managed-property>
		<managed-property>
			<property-name>label</property-name>
			<value>Definitions</value>
		</managed-property>
		<managed-property>
			<property-name>name</property-name>
			<value>projectTracking.definitions</value>
		</managed-property>
		<managed-property>
			<property-name>disabled</property-name>
			<value>false</value>
		</managed-property>
		<managed-property>
			<property-name>viewIDs</property-name>
			<list-entries>
				<value-class>java.lang.String</value-class>
				<value>/projectTracking/defCatalog.jsp</value>
			</list-entries>
		</managed-property>
		<managed-property>
			<property-name>outcome</property-name>
			<value>projectTracking.definitions</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>projectTrackingReference</managed-bean-name>
		<managed-bean-class>
			us.wy.state.deq.impact.webcommon.projectTracking.ProjectTrackingReference
		</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>createProject</managed-bean-name>
		<managed-bean-class>
			us.wy.state.deq.impact.webcommon.projectTracking.CreateProject
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>projectTrackingService</property-name>
			<value>#{projectTrackingBO}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>projectTrackingDetail</managed-bean-name>
		<managed-bean-class>
			us.wy.state.deq.impact.webcommon.projectTracking.ProjectTrackingDetail
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>projectTrackingService</property-name>
			<value>#{projectTrackingBO}</value>
		</managed-property>
		<managed-property>
			<property-name>infrastructureService</property-name>
			<value>#{infrastructureBO}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>projectTrackingSearch</managed-bean-name>
		<managed-bean-class>us.wy.state.deq.impact.webcommon.projectTracking.ProjectTrackingSearch</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>projectTrackingService</property-name>
			<value>#{projectTrackingBO}</value>
		</managed-property>	
	</managed-bean>
	
	<managed-bean>
	    <managed-bean-name>projectTrackingDefCatalog</managed-bean-name>
	    <managed-bean-class>
	      us.oh.state.epa.stars2.app.admin.DefCatalog
	    </managed-bean-class>
    	<managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>infrastructureService</property-name>
            <value>#{infrastructureBO}</value>
        </managed-property>
        <managed-property>
            <property-name>dataSet</property-name>
            <value>#{projectTrackingDataSet}</value>
        </managed-property>
        <managed-property>
            <property-name>defLoader</property-name>
            <value>#{projectTrackingDefLoader}</value>
        </managed-property>
  	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>projectTrackingDataSet</managed-bean-name>
		<managed-bean-class>
			us.oh.state.epa.stars2.database.adhoc.DataSet
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>infrastructureService</property-name>
			<value>#{infrastructureBO}</value>
		</managed-property>
	</managed-bean>

  	<managed-bean>
		<managed-bean-name>projectTrackingDefLoader</managed-bean-name>
		<managed-bean-class>
			us.oh.state.epa.stars2.app.admin.DefinitionsLoader
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>defConfigNode</property-name>
			<value>app.projectTracking-definitions</value>
		</managed-property>
	</managed-bean>

	<navigation-rule>
		<navigation-case>
			<from-outcome>projectTrackingSearch</from-outcome>
			<to-view-id>/projectTracking/projectTrackingSearch.jsp</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>projectTrackingDetail</from-outcome>
			<to-view-id>/projectTracking/projectTrackingDetail.jsp</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>createProjectTracking</from-outcome>
			<to-view-id>/projectTracking/createProjectTracking.jsp</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:projectTrackingEventDetail</from-outcome>
			<to-view-id>/projectTracking/projectTrackingEventDetail.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:projectNoteDetail</from-outcome>
			<to-view-id>/projectTracking/projectNoteDetail.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:projectAttachmentDetail</from-outcome>
			<to-view-id>/projectTracking/projectAttachmentDetail.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:budgetDetail</from-outcome>
			<to-view-id>/projectTracking/budgetDetail.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:prtProjectTrackingDetail</from-outcome>
			<to-view-id>/projectTracking/prtProjectTrackingDetail.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>projectTracking.definitions</from-outcome>
			<to-view-id>/projectTracking/defCatalog.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:projectTrackingEditDefinitionDialog</from-outcome>
			<to-view-id>/projectTracking/editDefinition.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:projectTrackingEditCustomDefinitionDialog</from-outcome>
			<to-view-id>/projectTracking/editCustomDefinition.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>