	<parameter name="DATE" isForPrompting="true" class="java.sql.Timestamp">
		<defaultValueExpression ><![CDATA[new java.sql.Timestamp(java.util.Calendar.getInstance().getTimeInMillis())]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT fp.facility_id, fp.facility_nm, pd.permit_type_dsc, pt.permit_nbr, 
       TO_CHAR(pi.issuance_date, 'MM/dd/YYYY') AS final_issuance_date
  FROM Stars2.fp_facility fp
  INNER JOIN Stars2.pt_permit pt
    ON (fp.facility_id = pt.facility_id)
  INNER JOIN Stars2.pt_permit_issuance pi
    ON (pt.permit_id = pi.permit_id
          AND pi.issuance_type_cd = 'F'
          AND pi.issuance_status_cd = 'I'
          AND SUBSTR(pi.issuance_date, 1, 9) = SUBSTR($P{DATE}, 1, 9))
  INNER JOIN Stars2.pt_permit_type_def pd
    ON (pt.permit_type_cd = pd.permit_type_cd)
  WHERE fp.version_id = -1
  ORDER BY fp.facility_id]]></queryString>

