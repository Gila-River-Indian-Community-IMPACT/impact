	<queryString>
		<![CDATA[SELECT ff.FACILITY_ID, ff.FACILITY_NM, fpcd.PERMIT_CLASSIFICATION_DSC,
	fosd.OPERATING_STATUS_DSC, pddd.PER_DUE_DATE_DSC, cr.REPORT_TYPE_CD,
	TO_CHAR(cr.PER_DUE_DATE , 'Mon dd, yyyy') AS PER_DUE_DATE,
        TO_CHAR(cr.SUBMITTED_DATE , 'Mon dd, yyyy') AS SUBMITTED_DATE
FROM STARS2.FP_FACILITY ff
LEFT JOIN STARS2.FP_OPERATING_STATUS_DEF fosd
    ON (ff.OPERATING_STATUS_CD = fosd.OPERATING_STATUS_CD)
LEFT JOIN STARS2.FP_PERMIT_CLASSIFICATION_DEF fpcd
    ON (ff.PERMIT_CLASSIFICATION_CD = fpcd.PERMIT_CLASSIFICATION_CD)
LEFT JOIN (SELECT pddx.FACILITY_ID, pddx.PER_DUE_DATE_CD, pddx.EFFECTIVE_DT,
                ROW_NUMBER() OVER(PARTITION BY pddx.FACILITY_ID
                                 ORDER BY pddx.EFFECTIVE_DT DESC) AS rn
            FROM STARS2.FP_PER_DUE_DATE_XREF pddx) cpddx
    ON (cpddx.FACILITY_ID = ff.FACILITY_ID AND cpddx.rn = 1)
LEFT JOIN STARS2.PA_PER_DUE_DATE_DEF pddd
    ON (pddd.PER_DUE_DATE_CD = cpddx.PER_DUE_DATE_CD)
LEFT OUTER JOIN STARS2.CR_COMPLIANCE_REPORT cr
    ON (ff.FACILITY_ID = cr.FACILITY_ID
        and cr.REPORT_TYPE_CD = 'per'
        and cr.PER_DUE_DATE_CD = cpddx.PER_DUE_DATE_CD
        and cr.DAPC_ACCEPTED = 'yes'
        AND to_char(cr.PER_DUE_DATE, 'yyyy') = to_char(sysdate, 'yyyy'))
WHERE ff.VERSION_ID = -1
AND cpddx.PER_DUE_DATE_CD != '0'
ORDER BY cr.SUBMITTED_DATE]]>
	</queryString>
