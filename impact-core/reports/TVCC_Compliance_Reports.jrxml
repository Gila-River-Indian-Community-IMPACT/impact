	<queryString><![CDATA[SELECT ff.FACILITY_ID, ff.FACILITY_NM, fpsd.PERMIT_STATUS_DSC, fpcd.PERMIT_CLASSIFICATION_DSC, 
	fosd.OPERATING_STATUS_DSC, ff.TV_CERT_REPORT_DUE_DATE, fyrc.EMISSIONS_RPT_CD, fyrc.YEAR, 
	cr.REPORT_TYPE_CD, cr.TVCC_REPORTING_YEAR, TO_CHAR(cr.SUBMITTED_DATE , 'Mon dd, yyyy') AS SUBMITTED_DATE
FROM STARS2.FP_FACILITY ff
LEFT JOIN STARS2.FP_OPERATING_STATUS_DEF fosd
    ON (ff.OPERATING_STATUS_CD = fosd.OPERATING_STATUS_CD)
LEFT JOIN STARS2.FP_PERMIT_CLASSIFICATION_DEF fpcd
    ON (ff.PERMIT_CLASSIFICATION_CD = fpcd.PERMIT_CLASSIFICATION_CD)
LEFT JOIN STARS2.FP_PERMIT_STATUS_DEF fpsd
    ON (ff.PERMIT_STATUS_CD = fpsd.PERMIT_STATUS_CD)
LEFT JOIN STARS2.FP_YEARLY_REPORTING_CATEGORY fyrc 
    ON (ff.FACILITY_ID = fyrc.FACILITY_ID)
LEFT OUTER JOIN STARS2.CR_COMPLIANCE_REPORT cr 
    ON (ff.FACILITY_ID = cr.FACILITY_ID 
	and cr.REPORT_TYPE_CD = 'tvcc' 
	and cr.DAPC_ACCEPTED = 'yes' 
	and cr.TVCC_REPORTING_YEAR = to_char(sysdate, 'yyyy')-1)
WHERE ff.VERSION_ID = -1
AND (fyrc.EMISSIONS_RPT_CD = 'tv' 
	OR fyrc.EMISSIONS_RPT_CD = 'smtv')
AND fyrc.YEAR = to_char(sysdate, 'yyyy')-1
AND ff.TV_CERT_REPORT_DUE_DATE is not null
ORDER BY cr.SUBMITTED_DATE]]></queryString>

