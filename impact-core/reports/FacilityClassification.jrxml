	<queryString><![CDATA[Select fp.facility_Id, fp.facility_nm,
do.do_laa_dsc, cc.county_nm, fp.emissions_rpt_cd,  pc.permit_classification_dsc
from 
fp_facility fp LEFT OUTER JOIN fp_permit_classification_def pc 
ON (fp.permit_classification_cd=pc.permit_classification_cd), 
fp_facility_address_xref fx, 
cm_address cd, cm_county_def cc, cm_do_laa_def do
where 
fp.do_laa_cd=do.do_laa_cd and
fx.fp_id=fp.fp_id and cd.county_cd=cc.county_cd 
and fx.address_id=cd.address_id
and fp.version_id=-1 and do.do_laa_cd like $P{DOLAA}
and cc.county_nm like $P{County}]]></queryString>

