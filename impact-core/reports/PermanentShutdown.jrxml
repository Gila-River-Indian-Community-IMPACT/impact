	<queryString><![CDATA[SELECT fp.facility_id, fp.facility_nm, ccd.county_nm, ccd.do_laa_cd, 
       erd.emissions_rpt_dsc, fosd.operating_status_dsc, 
       eu.epa_emu_id, eosd.operating_status_dsc as eu_operating_status, 
       to_char(eu.shutdown_dt, 'mm/dd/yyyy') as eu_shutdown_dt,
       esd.exempt_status_dsc
  FROM Stars2.fp_facility fp
    INNER JOIN Stars2.fp_facility_address_xref faxr ON (fp.fp_id = faxr.fp_id)
    INNER JOIN Stars2.cm_address ca 
      ON (faxr.address_id = ca.address_id AND ca.end_dt IS NULL)
    INNER JOIN Stars2.cm_county_def ccd ON (ca.county_cd = ccd.county_cd)
    INNER JOIN Stars2.fp_emissions_unit eu ON (fp.fp_id = eu.fp_id)
    INNER JOIN stars2.FP_YEARLY_REPORTING_CATEGORY yrc ON (yrc.facility_id = fp.facility_id
    	AND yrc.year = (SELECT MAX(year) from stars2.FP_YEARLY_REPORTING_CATEGORY yrc2
    					WHERE yrc2.facility_id = fp.facility_id))
  	INNER JOIN stars2.FP_EMISSION_REPORTING_DEF erd ON (yrc.emissions_rpt_cd = erd.emissions_rpt_cd)
  	INNER JOIN stars2.FP_OPERATING_STATUS_DEF fosd ON (fosd.operating_status_cd = fp.operating_status_cd)
  	INNER JOIN stars2.FP_OPERATING_STATUS_DEF eosd ON (eosd.operating_status_cd = eu.operating_status_cd)
  	LEFT OUTER JOIN stars2.FP_EXEMPT_STATUS esd ON (esd.exempt_status_cd = eu.exempt_status_cd)
      WHERE fp.version_id = -1
          AND LOWER(fp.operating_status_cd) = 'sd'
	  AND fp.do_laa_cd LIKE $P{DO_LAA_CD}
	  AND eu.shutdown_dt >= $P{FROM_DATE}
	  AND eu.shutdown_dt < $P{TO_DATE}
	  AND ca.county_cd LIKE $P{COUNTY}
    ORDER BY  fp.facility_id, eu.epa_emu_id]]></queryString>

