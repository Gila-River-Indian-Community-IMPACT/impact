		<defaultValueExpression ><![CDATA["%"]]></defaultValueExpression>
	</parameter>
	<parameter name="FROM_DATE" isForPrompting="true" class="java.sql.Timestamp"/>
	<parameter name="TO_DATE" isForPrompting="true" class="java.sql.Timestamp"/>
	<parameter name="COUNTY" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA["%"]]></defaultValueExpression>
