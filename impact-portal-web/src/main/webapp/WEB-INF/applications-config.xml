<?xml version="1.0" encoding="windows-1252"?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config xmlns="http://java.sun.com/JSF/Configuration">
  <!-- Menu items -->

  <managed-bean>
    <managed-bean-name>menuItem_applications</managed-bean-name>
    <managed-bean-class>
      us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem
    </managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    <managed-property>
      <property-name>beanName</property-name>
      <value>menuItem_applications</value>
    </managed-property>
    <managed-property>
      <property-name>label</property-name>
      <value>Application Detail</value>
    </managed-property>
    <managed-property>
      <property-name>name</property-name>
      <value>applications</value>
    </managed-property>
    <managed-property>
      <property-name>viewIDs</property-name>
      <list-entries>
        <value-class>java.lang.String</value-class>
        <value>/applications/applicationDetail.jsp</value>
        <value>/relocation/intToRelocDetail.jsp</value>
      </list-entries>
    </managed-property>
    <managed-property>
      <property-name>children</property-name>
      <list-entries>
        <value-class>
          us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem
        </value-class>
        <value>#{menuItem_appDetail}</value>
      </list-entries>
    </managed-property>
    <managed-property>
      <property-name>rendered</property-name>
      <value>false</value>
    </managed-property>
    <managed-property>
      <property-name>outcome</property-name>
      <value>method.applicationDetail.refresh</value>
    </managed-property>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>menuItem_appDetail</managed-bean-name>
    <managed-bean-class>
      us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem
    </managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    <managed-property>
      <property-name>beanName</property-name>
      <value>menuItem_appDetail</value>
    </managed-property>
    <managed-property>
      <property-name>label</property-name>
      <value>Application Detail</value>
    </managed-property>
    <managed-property>
      <property-name>name</property-name>
      <list-entries>
      	<value>applications.applicationDetail</value>
      	<value>/relocation/intToRelocDetail.jsp</value>
      </list-entries>
    </managed-property>
    <managed-property>
      <property-name>viewIDs</property-name>
      <list-entries>
        <value-class>java.lang.String</value-class>
        <value>/applications/applicationDetail.jsp</value>
      </list-entries>
    </managed-property>
    <managed-property>
      <property-name>outcome</property-name>
      <value>applicationDetail</value>
    </managed-property>
  </managed-bean>
  <!-- end of Menu items -->

  <!-- Backing beans -->
  <managed-bean>
    <managed-bean-name>applicationSearch</managed-bean-name>
    <managed-bean-class>
      us.oh.state.epa.stars2.portal.application.ApplicationSearch
    </managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
     <managed-property>
	     <property-name>applicationService</property-name>
	     <value>#{applicationBO}</value>
     </managed-property>
        <managed-property>
            <property-name>permitService</property-name>
            <value>#{permitBO}</value>
        </managed-property>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>applicationDetail</managed-bean-name>
    <managed-bean-class>
      us.oh.state.epa.stars2.portal.application.ApplicationDetail
    </managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
     <managed-property>
         <property-name>applicationService</property-name>
         <value>#{applicationBO}</value>
     </managed-property>    
             <managed-property>
            <property-name>permitService</property-name>
            <value>#{permitBO}</value>
        </managed-property>
                <managed-property>
            <property-name>infrastructureService</property-name>
            <value>#{infrastructureBO}</value>
        </managed-property>
                <managed-property>
            <property-name>facilityService</property-name>
            <value>#{facilityBO}</value>
        </managed-property>
                <managed-property>
            <property-name>contactService</property-name>
            <value>#{contactBO}</value>
        </managed-property>
  </managed-bean>
  <!-- end of Backing beans -->

  <!-- Navigation rules -->
  <navigation-rule>
	<navigation-case>
		<from-outcome>home.applications.applicationDetail</from-outcome>
		<to-view-id>/applications/homeApplicationDetail.jsp</to-view-id>
		<redirect />
	</navigation-case>  
    <navigation-case>
      <from-outcome>applicationSearch</from-outcome>
      <to-view-id>/applications/applicationSearch.jsp</to-view-id>
      <redirect/>
    </navigation-case>  
    <navigation-case>
      <from-outcome>applications</from-outcome>
      <to-view-id>/applications/applicationDetail.jsp</to-view-id>
      <redirect />
    </navigation-case>
    <navigation-case>
      <from-outcome>application_task_*</from-outcome>
      <to-view-id>/applications/applicationDetail.jsp</to-view-id>
      <redirect />
    </navigation-case>    
    <navigation-case>
      <from-outcome>applicationDetail</from-outcome>
      <to-view-id>/applications/applicationDetail.jsp</to-view-id>
      <redirect />
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:applicationDoc</from-outcome>
      <to-view-id>/applications/documentDetail.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:requiredApplicationDoc</from-outcome>
      <to-view-id>/applications/requiredDocumentDetail.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:tvAppFacWideReqDetail</from-outcome>
      <to-view-id>/applications/tvAppFacWideReqDetail.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:newApplication</from-outcome>
      <to-view-id>/applications/newApplication.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:createIntToRel</from-outcome>
      <to-view-id>/relocation/createNewITR.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>relocateDetail</from-outcome>
      <to-view-id>/relocation/intToRelocDetail.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:appDeleteDetail</from-outcome>
      <to-view-id>/applications/appDeleteDetail.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:scenarioDeleteDetail</from-outcome>
      <to-view-id>/applications/altScenarioDeleteDetail.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:euGroupDeleteDetail</from-outcome>
      <to-view-id>/applications/euGroupDeleteDetail.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:copyApplication</from-outcome>
      <to-view-id>/applications/copyApplication.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:appEmissionsInfo</from-outcome>
      <to-view-id>/applications/emissionsInfo.jsp</to-view-id>
    </navigation-case>
      <navigation-case>
      <from-outcome>dialog:appEmissionsFugLeaksOilGasInfo</from-outcome>
      <to-view-id>/applications/emissionsFugLeaksOilGasInfoDetail.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:appTVEmissionsInfo</from-outcome>
      <to-view-id>/applications/tvEmissionsInfo.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:appTVAppReqInfo</from-outcome>
      <to-view-id>/applications/tvApplicableReqsInfo.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:appSubmitDetail</from-outcome>
      <to-view-id>/applications/submitDetail.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:appConfirmTSDownload</from-outcome>
      <to-view-id>/applications/confirmTSDownload.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:tvAltScenarioPteReq</from-outcome>
      <to-view-id>/applications/tvPteReqInfo.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:appPrintableDocuments</from-outcome>
      <to-view-id>/applications/documentPrintInfo.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:appNoTsDocuments</from-outcome>
      <to-view-id>/applications/documentNoTs.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:associatedPermits</from-outcome>
      <to-view-id>/applications/permitInfo.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:appFacilityContacts</from-outcome>
      <to-view-id>/applications/facilityContacts.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:bactPollutantDetail</from-outcome>
      <to-view-id>/applications/bactPollutantDetail.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:laerPollutantDetail</from-outcome>
      <to-view-id>/applications/laerPollutantDetail.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:appDocTypeExplanations</from-outcome>
      <to-view-id>/applications/appDocTypeExplanations.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>dialog:tvAppDocTypeExplanations</from-outcome>
      <to-view-id>/applications/tvAppDocTypeExplanations.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
    <from-outcome>dialog:appTVPollutantLimit</from-outcome>
    <to-view-id>/applications/tvPollutantLimit.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
    <from-outcome>dialog:appTVOperationalRestriction</from-outcome>
    <to-view-id>/applications/tvOperationalRestriction.jsp</to-view-id>
    </navigation-case>
    	<navigation-case>
	    	<from-outcome>dialog:duplicateApplicationSubmission</from-outcome>
	    	<to-view-id>/applications/duplicateApplicationSubmissionWarning.jsp</to-view-id>
    	</navigation-case>    
  </navigation-rule>
  <!-- end of Navigation rules -->

</faces-config>
