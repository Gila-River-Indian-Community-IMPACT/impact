<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<!-- =========== Reports Tab CONFIGURATION FILE ================================== -->
<faces-config xmlns="http://java.sun.com/JSF/Configuration">
	<!--Reports Tab-->
	<!-- menu items -->
	<managed-bean>
		<managed-bean-name>menuItem_emissionsReport</managed-bean-name>
		<managed-bean-class>
			us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>menuItem_emissionsReport</value>
		</managed-property>
		<managed-property>
			<property-name>label</property-name>
			<value>Task - Emissions Inventory</value>
		</managed-property>
		<managed-property>
			<property-name>name</property-name>
			<value>emissionsReport</value>
		</managed-property>
		<managed-property>
			<property-name>viewIDs</property-name>
			<list-entries>
				<value-class>java.lang.String</value-class>
				<value>/reports/reportDetail.jsp</value>
				<value>/reports/condFiltWarning.jsp</value>
				<value>/reports/valTradSecNotif.jsp</value>
			</list-entries>
		</managed-property>
		<managed-property>
			<property-name>children</property-name>
			<list-entries>
				<value-class>
					us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem
				</value-class>
				<value>#{menuItem_TVDetail}</value>
			</list-entries>
		</managed-property>
		<managed-property>
			<property-name>rendered</property-name>
			<value>false</value>
		</managed-property>
		<managed-property>
			<property-name>outcome</property-name>
			<value>method.reportDetail.refresh</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>menuItem_TVDetail</managed-bean-name>
		<managed-bean-class>
			us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>menuItem_TVDetail</value>
		</managed-property>
		<managed-property>
			<property-name>label</property-name>
			<value>Emissions Inventory Detail</value>
		</managed-property>
		<managed-property>
			<property-name>name</property-name>
			<value>emissionsReport.TVdetail</value>
		</managed-property>
		<managed-property>
			<property-name>disabled</property-name>
			<value>true</value>
		</managed-property>
		<managed-property>
			<property-name>viewIDs</property-name>
			<list-entries>
				<value-class>java.lang.String</value-class>
				<value>/reports/reportDetail.jsp</value>
				<value>/reports/locatePollutant.jsp</value>
			</list-entries>
		</managed-property>
		<managed-property>
			<property-name>children</property-name>
			<list-entries>
				<value-class>
					us.oh.state.epa.stars2.webcommon.menu.SimpleMenuItem
				</value-class>
			</list-entries>
		</managed-property>
		<managed-property>
			<property-name>outcome</property-name>
			<value>TVDetail</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>reportSearch</managed-bean-name>
		<managed-bean-class>
			us.oh.state.epa.stars2.webcommon.reports.ReportSearch
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>infrastructureService</property-name>
            <value>#{infrastructureBO}</value>
        </managed-property>
        <managed-property>
            <property-name>emissionsReportService</property-name>
            <value>#{emissionsReportBO}</value>
        </managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>reportDetail</managed-bean-name>
		<managed-bean-class>
			us.oh.state.epa.stars2.portal.emissionsReport.ReportDetail
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>emissionsReportService</property-name>
            <value>#{emissionsReportBO}</value>
        </managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>erNTVDetail</managed-bean-name>
		<managed-bean-class>
			us.oh.state.epa.stars2.portal.emissionsReport.ErNTVDetail
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
		<!-- end menu items -->

	<!-- Backing beans -->
	<managed-bean>
		<managed-bean-name>reportProfile</managed-bean-name>
		<managed-bean-class>
			us.oh.state.epa.stars2.portal.emissionsReport.ReportProfile
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>emissionsReportService</property-name>
            <value>#{emissionsReportBO}</value>
        </managed-property>
        <managed-property>
            <property-name>facilityService</property-name>
            <value>#{facilityBO}</value>
        </managed-property>
	</managed-bean>

	<navigation-rule>
		<navigation-case>
			<from-outcome>dialog:createEUListPop</from-outcome>
			<to-view-id>/reports/processEG71Choices.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:createGroupPop</from-outcome>
			<to-view-id>/reports/processChoices.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:emissionsDoc</from-outcome>
			<to-view-id>/reports/documentDetail.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:emissionsDocNTV</from-outcome>
			<to-view-id>/reports/documentDetailNTV.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:rptConfirmTSDownload</from-outcome>
			<to-view-id>/reports/confirmTSDownload.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:rptConfirmTSDownloadNTV</from-outcome>
			<to-view-id>/reports/confirmTSDownloadNTV.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:selectReportData</from-outcome>
			<to-view-id>/reports/createReportData.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:reviseNtvReport</from-outcome>
			<to-view-id>/reports/reviseNTVReport.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>emissionReport</from-outcome>
			<to-view-id>/reports/reportDetail.jsp</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>emissionReportNoRedirect</from-outcome>
			<to-view-id>/reports/reportDetail.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:viewCalc</from-outcome>
			<to-view-id>/reports/explain.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/reports/reportDetail.jsp</from-view-id>
		<navigation-case>
			<from-outcome>dialog:confirmSaveReport</from-outcome>
			<to-view-id>/reports/confirmSaveReport.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:editContact</from-outcome>
			<to-view-id>/reports/addContact.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:deleteNtvReportReq</from-outcome>
			<to-view-id>/reports/deleteNtvReportReq.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:reviseReport</from-outcome>
			<to-view-id>/reports/reviseReport.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{reportDetail.editFIRE}</from-action>
			<from-outcome>"editFire"</from-outcome>
			<to-view-id>/reports/fireTable.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:editViewFire</from-outcome>
			<to-view-id>/reports/fireTable.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:deletePeriod</from-outcome>
			<to-view-id>/reports/deletePeriod.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:deleteEmissions</from-outcome>
			<to-view-id>/reports/deleteEmissions.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:deleteEmissionUnit</from-outcome>
			<to-view-id>/reports/deleteEmissionUnit.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:requestEditFP</from-outcome>
			<to-view-id>/reports/editFacilityProfile.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>goToFacilityPage</from-outcome>
			<to-view-id>/facilities/facilityProfile.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:tradeSecret</from-outcome>
			<to-view-id>/reports/tradeSecretJust.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:tradeSecretM</from-outcome>
			<to-view-id>/reports/tradeSecretMJust.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:tradeSecretSched</from-outcome>
			<to-view-id>/reports/tradeSecretSJust.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:tradeSecretES</from-outcome>
			<to-view-id>/reports/textBoxViewEdit.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:deleteReportReq</from-outcome>
			<to-view-id>/reports/deleteReportReq.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:associateProfile</from-outcome>
			<to-view-id>/reports/associateProfile.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/reports/homeReportDetail.jsp</from-view-id>
		<navigation-case>
			<from-outcome>dialog:tradeSecretM</from-outcome>
			<to-view-id>/reports/tradeSecretMJust.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/reports/createReport.jsp</from-view-id>
		<navigation-case>
			<from-outcome>failure</from-outcome>
			<to-view-id>/home.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>
				#{reportDetail.startCreateReportDone}
			</from-action>
			<to-view-id>/home.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<navigation-case>
			<from-outcome>home.reports.reportDetail</from-outcome>
			<to-view-id>/reports/homeReportDetail.jsp</to-view-id>
			<redirect />
		</navigation-case>  
		<navigation-case>
			<from-outcome>dialog:createNewReport</from-outcome>
			<to-view-id>/reports/createReport.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>create_facilityId</from-outcome>
			<to-view-id>/reports/createReportData.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>failure</from-outcome>
			<to-view-id>/home.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/home.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/reports/editFacilityProfile.jsp</from-view-id>
		<navigation-case>
			<from-outcome>failure</from-outcome>
			<to-view-id>/reports/reportDetail.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/reports/fireTable.jsp</from-view-id>
		<navigation-case>
			<from-action>#{reportDetail.saveFireSelection}</from-action>
			<to-view-id>/reports/reportDetail.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<navigation-case>
      		<from-outcome>dialog:EmiNoTsDocuments</from-outcome>
      		<to-view-id>/reports/documentNoTs.jsp</to-view-id>
    	</navigation-case>	
		<navigation-case>
			<from-outcome>dialog:emissionReport</from-outcome>
			<to-view-id>/reports/printEmiRpt.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:ntvEmissionReport</from-outcome>
			<to-view-id>/reports/printNtvEmiRpt.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>validationTradeSecretNotification</from-outcome>
			<to-view-id>/reports/valTradSecNotif.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>	
	<navigation-rule>
	<navigation-case>
			<from-outcome>dialog:confirmChangeMaterial</from-outcome>
			<to-view-id>/reports/confirmChangeMaterial.jsp</to-view-id>
	</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>dialog:dataEntryWizard</from-outcome>
			<to-view-id>/reports/dataEntryWizard.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog:confirmWizardLaunch</from-outcome>
			<to-view-id>/reports/confirmWizardLaunch.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
